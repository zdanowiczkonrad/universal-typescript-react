{
  "name": "universal-typescript-react",
  "version": "1.0.1",
  "description": "React + TypeScript + Webpack 4 + Karma + Enzyme + HMR",
  "scripts": {
    "**** DEVELOPMENT": "****",
    "start": "webpack-dev-server --mode development --config webpack.config.dev.babel.js",
    "start:no-babel": "webpack-dev-server --mode development --config webpack.config.dev.js",
    "styleguidist": "styleguidist server",
    "test:chrome": "karma start --reporters nyan,junit --single-run=true --browsers Chrome",
    "test:watch": "karma start --reporters nyan --single-run=false --browsers Chrome",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "**** PROD BUILD SCRIPTS": "****",
    "test": "karma start --reporters mocha,junit --single-run=true --browsers PhantomJS",
    "prebuild": "rimraf dist",
    "build": "webpack --mode production --config webpack.config.js  -p --env=prod",
    "**** SERVER SCRIPTS": "****",
    "server": "cd dist && python -m SimpleHTTPServer 8080",
    "ssr-server": "webpack --config webpack.config.server.js && node server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zdanowiczkonrad/universal-typescript-react.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "typescript",
    "ts",
    "webpack",
    "hmr",
    "karma",
    "starter"
  ],
